{
  "name": "isomorphic-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "",
    "client": "webpack-dev-server --config config/webpack/development.js --progress",
    "server": "nodemon src/server/index.js --exec babel-node --presets=env,react",
    "build:client": "NODE_ENV=production rm -rf public && webpack --config webpack.production.config.js --progress",
    "build:server": "NODE_ENV=production babel src -d build --presets=env,react",
    "prod:server": "NODE_ENV=production node build/server.js",
    "styleguide": "styleguidist server --config config/styleguidist/config.js",
    "styleguide:build": "styleguidist build --config config/styleguidist/config.js",
    "lint": "standard src/**/*.js",
    "cc": "node scripts/create-component"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chalk": "^1.1.3",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.15.2",
    "fs-promise": "^2.0.2",
    "inquirer": "^3.0.6",
    "jss": "^6.5.0",
    "jss-preset-default": "^1.3.1",
    "nodemon": "^1.11.0",
    "prop-types": "^15.5.4",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-helmet": "^5.0.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-jss": "^5.4.1",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "standard": "^10.0.1",
    "webpack": "^2.3.3",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "devDependencies": {
    "react-styleguidist": "^5.0.6"
  }
}
