{
  "name": "isomorphic-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:client": "webpack-dev-server --config webpack.development.config.js --progress",
    "start:server": "nodemon src/server.js --exec babel-node --presets=env,react",
